{
  "name": "uxe-candidate-exercise",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "react-scripts": "2.0.5",
    "rucksack-css": "^1.0.2",
    "tailwindcss": "^1.1.2"
  },
  "scripts": {
    "start": "npm-run-all -p watch:css watch:js",
    "build": "npm-run-all build:css build:js",
    "build:css": "postcss src/App.pcss -o src/App.css",
    "watch:css": "postcss src/App.pcss -o src/App.css -w",
    "build:js": "react-scripts build",
    "watch:js": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
